{
  "name": "ahmetcan.dev",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tailwind:build": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css",
    "tailwind:watch": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "cd \"./src/\" && eslint . --fix --ext .ts,.tsx,.js && cd ..",
    "fix:all": "npm run format && npm run lint:fix",
    "precommit": "npm run format && npm run lint:fix && npm run lint && npm run tsc",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,json}\"",
    "postinstall": "husky init",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.316.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.8.9"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
